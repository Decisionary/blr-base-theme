<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<parser>
		<target>docs/php/cache</target>
	</parser>
	<transformer>
		<target>docs/php</target>
	</transformer>
	<transformations>
		<template name="responsive-twig" />
	</transformations>
	<files>
		<directory>./</directory>
		<ignore>assets/*</ignore>
		<ignore>docs/*</ignore>
		<ignore>test/*</ignore>
		<ignore>bower_components/*</ignore>
		<ignore>node_modules/*</ignore>
		<ignore>vendor/*</ignore>
	</files>
</phpdoc>
