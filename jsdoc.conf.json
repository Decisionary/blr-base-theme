{
	"source": {
		"include": [
			"README.md",
			"assets/source/js/",
			"gulp-src/tasks"
		]
	},
	"plugins": [ "plugins/markdown" ],
	"templates": {
		"theme": "cosmo",
		"cleverLinks": true,
		"monospaceLinks": true,
		"includeDate": false,
		"linenums": true
	},
	"opts": {
		"template": "./node_modules/ink-docstrap/template",
		"destination": "./docs/js",
		"recurse": true
	}
}
